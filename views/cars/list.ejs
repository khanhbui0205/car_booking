  </div>

  <div class="card card-custom">
    <table class="table table-hover mb-0">
      <thead>
        <tr>
          <th>Name</th>
          <th>Brand</th>
          <th>Price</th>
        </tr>
      </thead>
      <tbody>
        <% cars.forEach(car => { %>
          <tr>
